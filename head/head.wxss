/* head/head.wxss */
.head{
  position: relative;
}
.header{
    position: relative;
    overflow: hidden;
    color: #fff;
    background: rgba(7,17,27,0.5);
}
.header .contnet-wrapper{
  display: flex;
  padding: 32rpx 20rpx;
}
.header .contnet-wrapper .content{
  margin-left: 20rpx;
  line-height: 44rpx;
}
.header .contnet-wrapper .content .name{
  font-size: 16px;
}
.header .contnet-wrapper .content .price,.header .contnet-wrapper .content .description{
  font-size: 12px;
}
.header .count{
  font-size:12px;
  position:absolute;
  bottom:27px;
  right:32px;
  background:rgba(0,0,0,0.2);
  padding:0px 9px;
  border-radius:14px;
  color:#fff;
  height:24px;
  line-height:24px;
  text-align:center;
}
.head .background{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom:0;
  z-index: -1;
  filter: blur(10px);
  width: 100%;
  height: 100%;
}
.bullet_wrapper{
  width: 100%;
  height: 28px;
  line-height: 28px;
  position: relative;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  background: rgba(0,0,0,0.4);
  font-size: 12px;
  color: #fff;
}
.bullet_wrapper .img{
  background:url('data:image/jpeg;base64,') no-repeat center;
  width: 20px;
  height: 20px;
  display:inline-block;
  vertical-align:middle;
  margin:4px 8px;

}
.detail{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom:0;
  background: rgba(7,17,27,0.5);
  width: 100%;
  height: 100%;
  z-index: 101;
}
.close{
  font-size: 20px;
  color: rgba(7,17,27,0.8);
  position: absolute;
  left: 50%;
  margin-left: -10px;
  bottom:10%;
 background:#fff;
  border-radius: 50%;
  width: 32px;
  height: 32px;
  line-height: 32px;
  text-align: center;
}
.detail_content{
  width: 92%;
  margin: 0 auto;
  text-align: center;
  color: #fff;
  font-size: 14px;
  margin-top: 64px;
  padding-bottom: 64px;
}
.detail_content .name{
  font-size: 16px;
  font-weight: bold;
}
.detail_content .detail_message{
  margin: 30px auto;
}
.detail_bulle{
  margin: 30px auto;
  display: flex;
}
.bulle_content{
  text-align: left;
  line-height: 26px;
  font-size: 12px;
}
.message_content{
  line-height: 28px;
  font-size: 12px;
}
.detail_message{
  display: flex;
}
.detail_content .line{
  flex: 1;
  border-bottom: 1px solid rgba(255,255,255,0.8);
  position: relative;
  top:-8px;
  margin:0 8px
}
